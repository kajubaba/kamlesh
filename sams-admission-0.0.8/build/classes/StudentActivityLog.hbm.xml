<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.narendra.sams.admission.domain.StudentActivityLog" table="student_activity_log">
		
		<id name="id">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="studentId" column="student_id"/>
		<property name="activityType" column="activity_type" />
		<property name="studentClassId" column="student_class_id"/>
		<property name="academicYearId" column="academic_year_id" />
		<property name="ceratedByUserId" column="user_id" />
		<property name="createdOn" column="created_date_time" />
		<property name="comments" column="user_remarks" />
		<property name="fromId" column="from_id" />
		<property name="toId" column="to_id" />
		
	</class> 
</hibernate-mapping>
